{
  "name": "jam",
  "description": "Monorepo for Josh Medeski",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "format": "prettier --write \"**/*.{ts,tsx,md,mdx,yml,yaml,js,json}\"",
    "lint": "turbo run lint",
    "ui": "yarn turbo run dev --scope=@jam/ui"
  },
  "devDependencies": {
    "prettier": "^2.5.1",
    "turbo": "^1.1.10"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "packageManager": "yarn@1.22.15",
  "version": "1.0.0"
}
